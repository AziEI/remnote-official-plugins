{
  "manifestVersion": 1,
  "id": "demo_autocomplete",
  "name": "Autocomplete",
  "author": "Jamesb",
  "repoUrl": "https://github.com/remnoteio/remnote-official-plugins",
  "enableOnMobile": false,
  "version": {
    "major": 0,
    "minor": 6,
    "patch": 0
  },
  "minimumRemNoteVersion": {
    "major": 1,
    "minor": 1,
